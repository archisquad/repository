{
  "name": "@archisquad/repository",
  "version": "0.0.1",
  "description": "Repository pattern implementation with TypeScript as framework agnostic library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "lint:dev": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "unbuild",
    "pub-check": "publint"
  },
  "keywords": [
    "repository",
    "framework agnostic",
    "archisquad"
  ],
  "maintainers": [
    {
      "name": "Mateusz GostaÅ„ski",
      "email": "mateusz.gostanski@gmail.com"
    }
  ],
  "license": "MIT",
  "browserslist": [
    "chrome > 87, firefox > 89, edge > 87, safari > 15.4, ios > 15.4, android > 87, ChromeAndroid > 87, FirefoxAndroid > 89"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:archisquad/repository.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@archisquad/prettier-config": "1.0.1",
    "@biomejs/biome": "1.4.1",
    "@faker-js/faker": "8.3.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@total-typescript/ts-reset": "0.5.1",
    "@vitest/coverage-istanbul": "1.0.4",
    "eslint": "8.55.0",
    "eslint-config-archisquad": "1.2.0",
    "jsdom": "23.0.1",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "publint": "^0.2.6",
    "type-fest": "4.8.3",
    "typescript": "5.3.3",
    "unbuild": "2.0.0",
    "valibot": "0.24.1",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "1.0.3",
    "zod": "3.22.4"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2"
  }
}
