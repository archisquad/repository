{
  "name": "@archisquad/repository",
  "version": "0.0.1",
  "description": "Repository pattern implementation with TypeScript as framework agnostic library",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "lint:dev": "biome check --apply-unsafe ./",
    "lint": "biome ci ./",
    "type-check": "tsc --noEmit",
    "test": "vitest run --coverage",
    "build": "unbuild",
    "pub-check": "publint"
  },
  "keywords": ["repository", "framework agnostic", "archisquad"],
  "maintainers": [
    {
      "name": "Mateusz GostaÅ„ski",
      "email": "mateusz.gostanski@gmail.com"
    }
  ],
  "license": "MIT",
  "browserslist": [
    "chrome > 87, firefox > 89, edge > 87, safari > 15.4, ios > 15.4, android > 87, ChromeAndroid > 87, FirefoxAndroid > 89"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:archisquad/repository.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@faker-js/faker": "8.4.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@vitest/coverage-istanbul": "1.3.1",
    "jsdom": "24.0.0",
    "lint-staged": "15.1.0",
    "publint": "0.2.7",
    "type-fest": "4.10.3",
    "typescript": "5.3.3",
    "unbuild": "2.0.0",
    "valibot": "0.29.0",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1",
    "zod": "3.22.4"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2"
  }
}
